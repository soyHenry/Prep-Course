let arreglo1= [1,2,3,6,9,78,95,66,44,77,254]
let arreglo2=[2,3,4,5,1,2]
let general= [...arreglo1,...arreglo2]
console.log(general)
let soloduplicado = general.filter((x)=> general[x+1]===general[x])
console.log(soloduplicado)


let arre=[2,3,4,5,1,2]
let arr = []
for(let i=0; i<arre.length; i++)
{
arr[i] = 2  * arre[i]
}console.log(arr)

const array = [1, 2, 3, 4];
let sum = 0;

for (let i = 0; i < array.length; i++) {
    sum += array[i];
}
console.log(sum);