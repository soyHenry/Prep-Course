

function capToFront(s) {
  let arr = [];
  for (let i in s) {
    if (s[i] === s[i].toUpperCase()) {
      arr.push(s[i]);
    }
  }
  for (let i in s) {
    if (s[i] !== s[i].toUpperCase()) {
      arr.push(s[i]);
    }
  }
  return arr.toString().replace(/,/g, '');
}
console.log(capToFront('soyHENRY'))


