let numero = 100
let aa = String(numero).split('').map((x=>{ return Number(x) })).reverse().join('')
console.log(numero+' '+aa)
if(numero == aa) console.log("Es el mismo "); else console.log('No es igual')
//console.log(aa)
let reso = aa.map((x)=>{ if(aa.at() == aa.at(-x)){return 'es bueno'}else return 'no es' })
console.log(reso)
if(aa.at()==aa.at(-1)){ console.log('Es bueno')} else console.log('No es')

let re = aa.reverse()
let aaa = re.join('')

console.log(aa)
console.log(aaa)

if(numero === aaa) { console.log('Es igual')}else console.log('No es igual')

