class Sum {

  constructor (operador,operador2) {
     this.operador  = operador
     this.operador2 = operador2 
  }
 sum(){return this.operador + this.operador2}
  }

  let su = new Sum(6,45)
  console.log(su.sum())

  class Resta extends Sum{

constructor(){
super(operador,operador2)
}
res(){return this.operador - this.operador2}
}

let re = new Sum (7,6)
console.log(re.res)