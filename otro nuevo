let numero = 505
let aa = String(numero).split('').map((x=>{ return Number(x) }))
console.log(aa)
let re = aa.reverse()
let aaa = re.join('')

console.log(aa)
console.log(aaa)

if(numero === aaa) { console.log('Es igual')}else console.log('No es igual')